<div class="container">

  <!-- Page content -->
  <div class="page-content">

    <!-- Main content -->
    <div class="content-wrapper clear">



      <div class="panel panel-flat">
        <div class="panel-heading">
          <h2>Listing All Events</h2>
        </div>


        <table id="datatable" class="table datatable-basic">
          <thead>
            <tr>
              <th>Name</th>
              <th class="text-center" style="width: 20px;"><i class="icon-arrow-down12"></i></th>
            </tr>
          </thead>
          <tbody>




            <% if  !@events.blank? %>
            <% @events.each do |event| %>
            <tr>
              <td><%= event.name %></td>

              <td>
                <%= link_to 'Show',  event_path(event) %>
              </td>
              

            </tr>
            <% end %>
            <% end %>




          </tbody>
        </table>
      </div>

      <!-- /marketing campaigns -->



    </div>
    <!-- /main content -->

  </div>
  <!-- /page content -->

</div>
<!-- /page container -->

